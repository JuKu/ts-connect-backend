{
  "name": "ts-connect-backend",
  "version": "0.0.1",
  "description": "The backend for the ts-connect-app",
  "type1": "module",
  "main": "web-api/src/index.js",
  "scripts": {
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "start": "tsc --rootDir ./src/ &&  node dist/web-api/app.js",
    "start-dev": "tsc --rootDir ./src/ &&  nodemon ./src/web-api/app.ts",
    "start-prod": "tsc --rootDir ./src/ &&  node dist/web-api/app.js",
    "start-notification-service": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuKu/ts-connect-backend.git"
  },
  "keywords": [
    "flutter-backend"
  ],
  "author": "Justin Kuenzel, JuKuSoft",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JuKu/ts-connect-backend/issues"
  },
  "homepage": "https://github.com/JuKu/ts-connect-backend#readme",
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.3.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "firebase-admin": "^10.0.2",
    "glob": "^7.2.0",
    "ini": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "logrocket": "^2.2.0",
    "mongodb": "^4.3.1",
    "mongoose": "^6.1.8",
    "path": "^0.12.7",
    "read-ini-file": "^3.0.1",
    "sanitize": "^2.1.0",
    "setimmediate": "^1.0.5",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.13",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
