{
  "name": "ts-connect-backend",
  "version": "0.0.1",
  "description": "The backend for the ts-connect-app",
  "type1": "module",
  "main": "web-api/src/index.js",
  "scripts": {
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "start": "tsc --rootDir ./src/ &&  node dist/web-api/app.js",
    "start-dev": "tsc --rootDir ./src/ &&  nodemon ./src/web-api/app.ts",
    "start-prod": "tsc --rootDir ./src/ &&  node dist/web-api/app.js",
    "start-notification-service": "node .",
    "test": "cross-env NODE_ENV=test jest --coverage --testTimeout=10000",
    "test-old": "jest",
    "lint": "eslint . --ext .ts --fix",
    "sonar": "sonar-scanner"
  },
  "jest-comment": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuKu/ts-connect-backend.git"
  },
  "keywords": [
    "flutter-backend"
  ],
  "author": "Justin Kuenzel, JuKuSoft",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JuKu/ts-connect-backend/issues"
  },
  "homepage": "https://github.com/JuKu/ts-connect-backend#readme",
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.3.1",
    "async-ratelimiter": "^1.3.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "glob": "^7.2.0",
    "gzip": "^0.1.0",
    "helmet": "^5.0.2",
    "ini": "^2.0.0",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^8.5.1",
    "logrocket": "^2.2.0",
    "mongodb": "^4.3.1",
    "mongoose": "^6.1.8",
    "path": "^0.12.7",
    "ratelimiter": "^3.4.1",
    "read-ini-file": "^3.0.1",
    "redis": "^4.0.3",
    "sanitize": "^2.1.0",
    "setimmediate": "^1.0.5",
    "socket.io": "^4.4.1",
    "uuid": "^8.3.2",
    "validator": ">=13.7.0",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/ioredis": "^4.28.8",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.13",
    "@types/redis": "^4.0.11",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-security-node": "^1.1.1",
    "firebase-admin": "^10.0.2",
    "jest": "^27.5.1",
    "jest-sonar-reporter": "^2.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "sonar-scanner": "^3.1.0",
    "sonarqube-scanner": "^2.8.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "jest-old": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  },
  "jestSonar-old": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
